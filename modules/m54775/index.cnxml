                                </m:mrow>
                                <m:mrow>
                                  <m:mn>2</m:mn>
                                </m:mrow>
                              </m:msubsup>
                            </m:mrow>
                            <m:mrow><m:mn>2</m:mn><m:mi>a</m:mi></m:mrow>
                          </m:mfrac>
                        </m:mrow>
                      <m:mo stretchy="false">=</m:mo>
                      <m:mn>0 m</m:mn>
