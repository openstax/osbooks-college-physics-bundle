                            </m:mrow>
                          </m:msub>
                          <m:mo stretchy="false">+</m:mo>
                          <m:mfrac>
                            <m:mrow>
                              <m:msup>
                                <m:mi>v</m:mi>
                                <m:mrow>
                                  <m:mn>2</m:mn>
                                </m:mrow>
                              </m:msup>
                              <m:mo stretchy="false">−</m:mo>
