                        <m:mi>y</m:mi>
                        <m:mo stretchy="false">−</m:mo>
                        <m:msub>
                          <m:mi>y</m:mi>
                            <m:mn>0</m:mn>
                        </m:msub>
                  </m:mrow></m:mtd>
                </m:mtr>
                <m:mtr><m:mtd>                        <m:mi>y</m:mi></m:mtd>
<m:mtd>                        <m:mo stretchy="false">=</m:mo></m:mtd>
<m:mtd>
                  <m:mrow>
                    <m:mrow>
                      <m:mrow>
                        <m:mrow>
                          <m:msub>
