    <para id="import-auto-id909629"><m:math>
        
          <m:mrow>

              <m:mrow>
                <m:mrow>
                  <m:mi>ω</m:mi>
                  <m:mo stretchy="false">=</m:mo>

                    <m:mrow>
                      <m:mn>0</m:mn>
                      <m:mtext>.</m:mtext>
                      <m:mtext>737 rev/s</m:mtext>
                    </m:mrow>

                </m:mrow>
              </m:mrow>

