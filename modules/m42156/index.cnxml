                      </m:mrow>
                    </m:mfenced>

</m:mtd>
                </m:mtr>
                <m:mtr>
<m:mtd>

</m:mtd>
<m:mtd>
                   <m:mo stretchy="false">=</m:mo>
</m:mtd>
<m:mtd>
                    <m:mn>3</m:mn>
                    <m:mtext>.306 kg</m:mtext>
                      <m:mo stretchy="false">·</m:mo>
                      <m:mtext>m/s</m:mtext>
                            <m:mo stretchy="false">≈</m:mo>
                       <m:mtext>3.3 kg</m:mtext>
                      <m:mo stretchy="false">·</m:mo>
                      <m:mtext>m/s</m:mtext>

</m:mtd>
                </m:mtr>
              </m:mtable>
