      </m:mrow>
      <m:mrow>
       <m:mn>234</m:mn>
      </m:mrow>
     </m:mmultiscripts>
     <m:mtext>a</m:mtext><m:mo>+</m:mo><m:mmultiscripts>
      <m:mtext>H</m:mtext>
      <m:mprescripts/>
      <m:mn>2</m:mn>
      <m:mn>4</m:mn>
     </m:mmultiscripts>
     <m:mtext>e</m:mtext>
    </m:mrow>
    
  </m:math>
  </item>
  <item><m:math>
   
    <m:mrow>
     <m:mmultiscripts>
      <m:mtext>R</m:mtext>
      <m:mprescripts/>
      <m:mrow>
       <m:mn>88</m:mn>
      </m:mrow>
      <m:mrow>
       <m:mn>223</m:mn>
      </m:mrow>
     </m:mmultiscripts>
     <m:mtext>a</m:mtext><m:mo stretchy="false">→</m:mo><m:mmultiscripts>
      <m:mtext>P</m:mtext>
      <m:mprescripts/>
      <m:mrow>
       <m:mn>82</m:mn>
      </m:mrow>
      <m:mrow>
       <m:mn>209</m:mn>
      </m:mrow>
     </m:mmultiscripts>
     <m:mtext>b</m:mtext><m:mo>+</m:mo><m:mmultiscripts>
      <m:mtext>C</m:mtext>
      <m:mprescripts/>
      <m:mn>6</m:mn>
      <m:mrow>
       <m:mn>14</m:mn>
      </m:mrow>
     </m:mmultiscripts>
  
    </m:mrow>
    
  </m:math></item>
  <item><m:math>
   
    <m:mrow>
     <m:mmultiscripts>
      <m:mtext>C</m:mtext>
      <m:mprescripts/>
      <m:mn>6</m:mn>
      <m:mrow>
       <m:mn>14</m:mn>
      </m:mrow>
     </m:mmultiscripts>
     <m:mo stretchy="false">→</m:mo><m:mmultiscripts>
      <m:mtext>N</m:mtext>
      <m:mprescripts/>
      <m:mn>7</m:mn>
      <m:mrow>
       <m:mn>14</m:mn>
      </m:mrow>
     </m:mmultiscripts>
     <m:mo>+</m:mo><m:msup>
      <m:mi>e</m:mi>
      <m:mo>−</m:mo>
     </m:msup>
     <m:mo>+</m:mo><m:msub>
      <m:mover accent="true">
       <m:mi>v</m:mi>
       <m:mo>¯</m:mo>
      </m:mover>
  
      <m:mtext>e</m:mtext>
     </m:msub>
  
    </m:mrow>
    
   
  </m:math>
  </item>
  <item><m:math>
   
    <m:mrow>
     <m:mmultiscripts>
      <m:mtext>M</m:mtext>
      <m:mprescripts/>
      <m:mrow>
       <m:mn>12</m:mn>
      </m:mrow>
      <m:mrow>
