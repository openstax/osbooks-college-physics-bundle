        <m:mo stretchy="false">(</m:mo><m:mn>50</m:mn><m:mtext>N</m:mtext><m:mo>⋅</m:mo><m:msup>
         <m:mtext>m</m:mtext>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>1</m:mn>
         </m:mrow>
        </m:msup>
        <m:mo stretchy="false">)</m:mo>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>
  
     </m:mrow>
    </m:mfrac>
    <m:mo>)</m:mo><m:mo>)</m:mo>
